{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection" : "Host=localhost;Port=5432;Database=TestToeic;User Id=postgres;Password=123;"
  },
  "ReverseProxy": {
    "Routes": {
      "nuxtRoute": {
        "ClusterId": "nuxtCluster",
        "Match": {
          "Path": "/{**catch-all}"  
        }
      }
    },
    "Clusters": {
      "nuxtCluster": {
        "Destinations": {
          "nuxtDestination": {
            "Address": "http://localhost:3000/"  
          }
        }
      }
    }
  },

  "AllowedHosts": "*"
}
